{
  "name": "javascript-starter",
  "version": "2.1.0",
  "description": "A starter kit for building web projects with modern JavaScript",
  "author": {
    "name": "Amit Agarwal",
    "email": "amit@labnol.org",
    "url": "https://digitalinspiration.com/"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "format": "prettier --write src/**/*.{js,json,md}",
    "build": "parcel build",
    "start": "parcel"
  },
  "keywords": [
    "javascript",
    "webpack",
    "eslint",
    "prettier",
    "parcel",
    "es6",
    "babel",
    "vscode"
  ],
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "includeNodeModules": true,
      "optimize": true,
      "distDir": "./dist",
      "sourceMap": false
    }
  },
  "engines": {
    "node": ">16"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@parcel/transformer-sass": "^2.7.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "node-sass": "^7.0.1",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
