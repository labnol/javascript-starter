{
  "name": "javascript-starter",
  "version": "2.0.1",
  "description": "A starter kit for building web projects with modern JavaScript",
  "author": {
    "name": "Amit Agarwal",
    "email": "amit@labnol.org",
    "url": "https://digitalinspiration.com/"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "format": "prettier --write src/**/*.{js,json,md}",
    "build": "parcel build",
    "start": "parcel"
  },
  "keywords": [
    "javascript",
    "webpack",
    "eslint",
    "prettier",
    "parcel",
    "es6",
    "babel",
    "vscode"
  ],
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "includeNodeModules": true,
      "optimize": true,
      "distDir": "./dist",
      "sourceMap": false
    }
  },
  "engines": {
    "node": ">10"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@parcel/transformer-sass": "^2.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "node-sass": "^6.0.1",
    "parcel": "^2.0.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
